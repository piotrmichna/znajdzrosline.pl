[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=homster
Group=www-data
WorkingDirectory=/home/homster/znajdzrosline/wdp
ExecStart=/home/homster/znajdzrosline/venv/bin/gunicorn --access-logfile - --workers 1 --bind unix:/home/homster/znajdzrosline/wdp/wdp.sock wdp.wsgi:application

[Install]
WantedBy=multi-user.target
